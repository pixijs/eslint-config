{
  "name": "@pixi/eslint-config",
  "version": "4.0.1",
  "description": "ESLint config for PixiJS plugin creators",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixijs/eslint-config.git"
  },
  "keywords": [
    "pixi.js",
    "pixijs",
    "eslint-config"
  ],
  "author": "Shukant K. Pal <shukantpal@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pixijs/eslint-config/issues"
  },
  "homepage": "https://github.com/pixijs/eslint-config#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache --ext .js . --ignore-path .gitignore --max-warnings 0",
    "lintfix": "npm run lint -- --fix",
    "test": "npm run lint"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^10.0.0"
  },
  "devDependencies": {
    "eslint": ">=8",
    "typescript": ">=5"
  },
  "peerDependencies": {
    "eslint": ">=8",
    "typescript": ">=5"
  }
}
